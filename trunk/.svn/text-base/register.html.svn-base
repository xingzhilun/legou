        <!-- 顶部 -->
        <div class="top clear cfff">
            <!-- 版心 -->
            <div class="container">
                <img src="./images/06-注册_02.png" alt="" class="fl bgcf60">
                <a href="#">legouchina.cn</a>
                <div class="fr loginbox">欢迎光临<a href="#">乐购</a>，请<span><a href="#">登陆</a></span>\<a href="#">注册</a>
                </div>
                <div class="title clearfix">
                    <p class="c666 fs30 fl">欢迎登陆</p>
                    <div class="title-dl ca6 fs24 fr">已有账号，请 <a href="#">登录</a></div>
                </div>
            </div>
        </div>
    </div>
    <!-- 内容 -->
    <div id="content">
        <div class="container tac">
        <form action="#" method="post" class="myform">
            <p>
              <label for="username">用户名</label>
              <input id="username" name="username" type="text" placeholder="您的账户名和登录名"> 
            </p>
            <p>
              <label for="password">设置密码</label>
              <input id="password" name="password" type="password" placeholder="建议设置两种字符密码">
            </p>
            <p>
              <label for="confirm_password">确认密码</label>
              <input id="confirm_password" name="confirm_password" type="password"  placeholder="再次输入密码">
            </p>
            <p>
              <label for="lastname">中国+86</label>
              <input id="lastname" name="lastname" type="text" placeholder="建议使用常用手机号">
            </p>
            <p>
                <label for="yz">验证码</label>
                <input id="lastname" name="lastname" type="text" placeholder="请输入验证码" >
                <i>W8GA</i>
            </p>
              <p>
                <label for="tel">手机验证码</label>
                <input id="tel" name="tel" type="tel" placeholder="输入手机验证码">
                <em>获取验证码</em>
            </p>
            <p>
                <input type="checkbox"  id="checkbox" name="checkbox" style="width: 12px;height: 12px;">
            <span>我已阅读并同意<a href="#" class="cf60">《京东用户注册协议》</a></span>
            </p>
              <p>
                <input class="submit cfff bgcf60 " type="submit" value="立即注册" style="text-indent: 0;">
              </p>
        </form>
    </div></div>
